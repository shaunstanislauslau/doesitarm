{
  "name": "doesitarm",
  "version": "1.0.0",
  "description": "Apps that support Apple Silicon",
  "author": "Sam Carlton",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "rm -f ./app-list.json && npm run clone-readme && nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "clone-readme": "cp ./README.md README-temp.md"
  },
  "dependencies": {
    "@nuxtjs/sitemap": "^2.4.0",
    "axios": "^0.21.0",
    "cross-env": "^5.2.0",
    "markdown-it": "^11.0.1",
    "observe-element-in-viewport": "0.0.15",
    "scroll-into-view-if-needed": "^2.2.26",
    "slugify": "^1.4.5",
    "typeface-inter": "^3.15.0"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^3.2.0",
    "autoprefixer": "^8.6.4",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.11.0",
    "nuxt": "^3.4.3",
    "prettier": "1.14.3",
    "tailwindcss": "^1.9.6"
  }
}
